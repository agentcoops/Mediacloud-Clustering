# get stop word hash from tiny list (~150)
sub get_tiny_stop_word_lookup {
    
    if ($_tiny_stop_words) {
        return $_tiny_stop_words;
    }
    
    $_tiny_stop_words = Lingua::StopWords::getStopWords('en');

    return $_tiny_stop_words;
}
