# return stop word hash with stop words stemmed by Lingua::Stem
sub get_tiny_stop_stem_lookup {
    
    if ($_tiny_stop_stems) {
        return $_tiny_stop_stems;
    }
    
    my $stop_words = get_tiny_stop_word_lookup();
    
    my $stems = Lingua::Stem::En::stem({ -words => [ keys(%{$stop_words}) ],
                                         -locale => 'en' });
    
    for my $stem (@{$stems}) {
        $_tiny_stop_stems->{$stem} = 1;
    }

    return $_tiny_stop_stems;
}

